depends=(':macos')
makedepends=(':xcode')

###### Install ######
# xcodebuild "-target", "mogenerator", "-configuration", "Release", "SYMROOT=symroot", "OBJROOT=objroot"
# bin.install "symroot/Release/mogenerator"
